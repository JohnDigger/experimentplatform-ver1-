<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mybatis.test.demo.mapper.auto.AccountMapper">

    <select id="selectUserAccountList" resultType="com.mybatis.test.demo.model.auto.UserAccount">
    select u.*,a.MONEY
    from
    user1 u,account a where u.id = a.UID

    </select>

</mapper>